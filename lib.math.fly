def mod
  fn |n, k|
    or
      n < k
      n

      n == k
      0

      n > k
      call mod
        n - k
        k
      end
    end
  end
end

def factorial
  fn |n|
    or
      n == 0
      1

      n > 0
      do
        val inner
          call factorial
            n - 1
          end
        end

        n * inner
      end
    end
  end
end